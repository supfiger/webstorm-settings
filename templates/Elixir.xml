<templateSet group="Elixir">
  <template name="cl" value="CoinHunter.inspect($VAR$)" shortcut="SPACE" description="" toReformat="false" toShortenFQNames="true">
    <variable name="VAR" expression="" defaultValue="" alwaysStopAt="true" />
    <context>
      <option name="ELIXIR_CODE" value="true" />
    </context>
  </template>
  <template name="@doc" value="@doc &quot;&quot;&quot;&#10;  ## $1$&#10;&quot;&quot;&quot;" description="@doc &quot;&quot;&quot; ... &quot;&quot;&quot;" toReformat="true" toShortenFQNames="true">
    <variable name="1" expression="" defaultValue="" alwaysStopAt="true" />
  </template>
  <template name="mdoc" value="@moduledoc &quot;&quot;&quot;&#10;$1$&#10;&quot;&quot;&quot;&#10;$END$" description="@moduledoc &quot;&quot;&quot; ... &quot;&quot;&quot;" toReformat="true" toShortenFQNames="true">
    <variable name="1" expression="" defaultValue="" alwaysStopAt="true" />
  </template>
  <template name="c" value="&amp;($CODE$)" shortcut="SPACE" description="" toReformat="false" toShortenFQNames="true">
    <variable name="CODE" expression="" defaultValue="\&amp;1" alwaysStopAt="true" />
    <context>
      <option name="ELIXIR_CODE" value="true" />
    </context>
  </template>
  <template name="ok" value="{:ok, $VAR$}" description="" toReformat="false" toShortenFQNames="true">
    <variable name="VAR" expression="" defaultValue="" alwaysStopAt="true" />
    <context>
      <option name="ELIXIR_CODE" value="true" />
    </context>
  </template>
  <template name="err" value="{:error, $VAR$}" description="" toReformat="false" toShortenFQNames="true">
    <variable name="VAR" expression="" defaultValue="" alwaysStopAt="true" />
    <context>
      <option name="ELIXIR_CODE" value="true" />
    </context>
  </template>
  <template name="forc" value="for $A$ &lt;- $B$ do&#10;  $END$&#10;end" description="for a &lt;- b  do .. end" toReformat="true" toShortenFQNames="true">
    <variable name="A" expression="" defaultValue="" alwaysStopAt="true" />
    <variable name="B" expression="" defaultValue="" alwaysStopAt="true" />
    <context>
      <option name="ELIXIR_CODE" value="true" />
    </context>
  </template>
  <template name="ifi" value="if $TRUE$, do: $END$" description="inline if" toReformat="false" toShortenFQNames="true">
    <variable name="TRUE" expression="" defaultValue="" alwaysStopAt="true" />
    <context>
      <option name="ELIXIR_CODE" value="true" />
    </context>
  </template>
  <template name="ifc" value="if $TRUE$ do&#10;  $END$&#10;end" description="if .. do .. end" toReformat="true" toShortenFQNames="true">
    <variable name="TRUE" expression="" defaultValue="" alwaysStopAt="true" />
    <context>
      <option name="ELIXIR_CODE" value="true" />
    </context>
  </template>
  <template name="do" value="do&#10;  $END$&#10;end" shortcut="TAB" description="do ... end" toReformat="true" toShortenFQNames="true" />
  <template name="try" value="try do&#10;  $1$&#10;rescue&#10;  $2$ -&gt; $END$&#10;" description="try ... rescue .. end" toReformat="true" toShortenFQNames="true" deactivated="true">
    <variable name="1" expression="" defaultValue="" alwaysStopAt="true" />
    <variable name="2" expression="" defaultValue="" alwaysStopAt="true" />
  </template>
  <template name="doc" value="@doc &quot;&quot;&quot;&#10;$1$&#10;&quot;&quot;&quot;&#10;$END$" description="@doc &quot;&quot;&quot; ... &quot;&quot;&quot;" toReformat="true" toShortenFQNames="true" deactivated="true">
    <variable name="1" expression="" defaultValue="" alwaysStopAt="true" />
  </template>
  <template name="map" value="Enum.map(&amp;($VAR$))" shortcut="SPACE" description="" toReformat="false" toShortenFQNames="true">
    <variable name="VAR" expression="" defaultValue="" alwaysStopAt="true" />
    <context>
      <option name="ELIXIR_CODE" value="true" />
    </context>
  </template>
  <template name="p" value="|&gt; " shortcut="SPACE" description="" toReformat="false" toShortenFQNames="true">
    <context>
      <option name="ELIXIR_CODE" value="true" />
    </context>
  </template>
  <template name="gl" value="Leverex.toggle()" shortcut="SPACE" description="" toReformat="false" toShortenFQNames="true">
    <context>
      <option name="ELIXIR_CODE" value="true" />
    </context>
  </template>
  <template name="pry" value="require IEx; IEx.pry" shortcut="SPACE" description="" toReformat="false" toShortenFQNames="true">
    <context>
      <option name="ELIXIR_CODE" value="true" />
    </context>
  </template>
  <template name="tg" value="Leverex.toggle()" shortcut="SPACE" description="" toReformat="false" toShortenFQNames="true">
    <context>
      <option name="ELIXIR_CODE" value="true" />
    </context>
  </template>
  <template name="st" value="Leverex.step()" shortcut="SPACE" description="" toReformat="false" toShortenFQNames="true">
    <context>
      <option name="ELIXIR_CODE" value="true" />
    </context>
  </template>
</templateSet>